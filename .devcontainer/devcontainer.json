{
  "name": "Python 3 with Jupyter",
  "image": "mcr.microsoft.com/devcontainers/python:3",
  "features": {
    "ghcr.io/devcontainers/features/python": {
      "version": "3.10"
    },
    "ghcr.io/devcontainers-contrib/features/jupyterlab": {}
